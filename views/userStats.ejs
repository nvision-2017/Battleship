<!DOCTYPE html>
<html>
<head>
	<title>Users</title>
</head>
<body>
<style type="text/css">
	table, th, td {
		padding: 5px;
	    border: 1px solid black;
	    border-collapse: collapse;
	}
</style>
	<table border="1">
		<thead>
			<tr>
				<td>Sl. No.</td>
				<td>Username</td>
				<td>Name</td>
				<td>Games Played(with disconnection)</td>
				<td>Email</td>
				<td>G+</td>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0 ; i<users.length ; i++) { %>

				<tr>
					<td><%=i+1%></td>
					<td><%=users[i].username%></td>
					<td><%=users[i].google.displayName%></td>
					<td><%=users[i].logs.length%></td>
					<td><%=users[i].email%></td>
					<td><a href="<%=users[i].google.profileUrl%>" target="_blank">G+</a></td>
				</tr>

			<% } %>
		</tbody>
	</table>

</body>
</html>